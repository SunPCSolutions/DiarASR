# Environment variables for diarasr API service

# API Keys for authentication (comma-separated list)
API_KEYS=<diarasr API key>

# Hugging Face token for accessing private models (required for Pyannote diarization)
HF_TOKEN=<your hf token>

# Logging level
LOG_LEVEL=INFO

# Maximum file size for uploads (MB)
MAX_FILE_SIZE_MB=100